<h1>Sign up</h1>
<%= form_with(model: @user, local: true) do |f| %>
<ul>
  <li>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </li>
  <li>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </li>
  <li>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </li>
  <li>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </li>
</ul>
  <%= f.submit "Create my account" %>
<% end %>
